# ===========================================
dist/
build/
*.tsbuildinfo

# ===========================================
# Testing & Coverage
# ===========================================
coverage/
.nyc_output
*.lcov

# ===========================================
# Logs
# ===========================================
logs/
*.log
bot.log
error.log

# ===========================================
# Runtime & Cache
# ===========================================
.cache/
.parcel-cache/
*.pid
*.seed
*.pid.lock

# ===========================================
# IDE & Editor
# ===========================================
.vscode/
.idea/
*.swp
*.swo
*~

# ===========================================
# OS Files
# ===========================================
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db

# ===========================================
# Temporary Files
# ===========================================
tmp/
temp/
*.tmp
*.temp
*.bak
*.backup

